{
  "name": "tinkoff-robot",
  "version": "1.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint src scripts --ext .ts",
    "tsc": "tsc --noEmit",
    "test": "echo 'tbd'",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "accounts": "ts-node-esm scripts/show-accounts.ts",
    "candles": "ts-node-esm scripts/load-candles.ts",
    "create-account": "ts-node-esm scripts/create-account.ts",
    "instruments": "ts-node-esm scripts/load-instruments.ts",
    "stats": "ts-node-esm scripts/get-stats.ts",
    "backtest": "ts-node-esm scripts/run-backtest.ts",
    "chart": "open chart/index.html",
    "toc": "markdown-toc README.md -i",
    "market": "ts-node-esm scripts/run-market.ts",
    "market:real": "ts-node-esm scripts/run-market.ts --real",
    "deploy": "./scripts/deploy.sh"
  },
  "lint-staged": {
    "{src,scripts}/**/*.ts": "eslint --ext .ts"
  },
  "dependencies": {
    "@debut/indicators": "^1.3.15",
    "@vitalets/logger": "^2.1.1",
    "kleur": "^4.1.5",
    "mockdate": "^3.0.5",
    "prompts": "^2.4.2",
    "tinkoff-invest-api": "^2.4.7",
    "yandex-cloud-fn": "^2.5.5",
    "yargs": "^17.7.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/eslint": "8.44.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.18",
    "@types/prompts": "^2.4.2",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-mocha": "^10.1.0",
    "lint-staged": "^12.5.0",
    "markdown-toc": "^1.2.0",
    "mocha": "^10.2.0",
    "nice-grpc": "^2.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "yandex-cloud-deploy-fn": "^2.0.0"
  },
